{
  "name": "crcms/framework",
  "description": "The CrCms Framework.",
  "keywords": [
    "crcms",
    "laravel",
    "swoole",
    "rpc",
    "microservice",
    "php"
  ],
  "license": "MIT",
  "type": "framework",
  "require": {
    "php": "^7.1.3",
    "doctrine/dbal": "^2.8",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.7.*",
    "laravel/tinker": "^1.0",
    "predis/predis": "^1.1",
    "crcms/repository": "dev-master"
  },
  "require-dev": {
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "CrCms\\Foundation\\": "src/"
    },
    "files": [
      "src/App/Helpers/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CrCms\\Foundation\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
